{
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": "error",
    "sort-keys": ["error"],
    "import/prefer-default-export": "off",
    "import/no-default-export": "error",
    "import/no-cycle": "off"
  }
}
