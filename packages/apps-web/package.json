    "link": "npm ln",
    "postbuild": "tsc --emitDeclarationOnly --outDir build/types",
    "start": "node ../../esbuild.server.mjs"
  },
  "devDependencies": {
    "@craftamap/esbuild-plugin-html": "^0.4.0",
    "node-html-parser": "^5.4.2-0"
  },
  "dependencies": {
    "@vaadin/router": "^1.7.4",
    "@galacticcouncil/apps": "^2.4.4",
    "@galacticcouncil/ui": "^2.0.3",
    "@galacticcouncil/xcm-cfg": "^1.3.0",
    "@galacticcouncil/xcm-sdk": "^2.1.0",
    "@polkadot/api": "^10.9.1",
    "@polkadot/extension-inject": "^0.44.6",
    "graphql-request": "^6.1.0"
  }
}
